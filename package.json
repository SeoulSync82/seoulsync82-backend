{
  "name": "seoulsync82-backend",
  "version": "0.3.0",
  "description": "SeoulSync82 backend API for AI-driven course recommendations.",
  "author": "Yoo SeungMo (https://github.com/yooseungmo)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "NODE_ENV=debug nest start --debug --watch",
    "start:staging": "NODE_ENV=staging node dist/main",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint '**/*.{js,ts}' --quiet",
    "lint:fix": "eslint '**/*.{js,ts}' --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --testRegex='.*\\integration.spec\\.ts$' --passWithNoTests",
    "test:e2e": "NODE_ENV=test jest --config ./jest-e2e.json --passWithNoTests",
    "test:user": "jest --testPathPattern=src/user --coverage --collectCoverageFrom=src/user/**",
    "test:user:watch": "jest --watch --testPathPattern=src/user --coverage --collectCoverageFrom=src/user/**",
    "test:auth": "jest --testPathPattern=src/auth --coverage --collectCoverageFrom=src/auth/**",
    "test:auth:watch": "jest --watch --testPathPattern=src/auth --coverage --collectCoverageFrom=src/auth/**",
    "test:bookmark": "jest --testPathPattern=src/bookmark --coverage --collectCoverageFrom=src/bookmark/**",
    "test:bookmark:watch": "jest --watch --testPathPattern=src/bookmark --coverage --collectCoverageFrom=src/bookmark/**",
    "test:comment": "jest --testPathPattern=src/comment --coverage --collectCoverageFrom=src/comment/**",
    "test:comment:watch": "jest --watch --testPathPattern=src/comment --coverage --collectCoverageFrom=src/comment/**",
    "test:community": "jest --testPathPattern=src/community --coverage --collectCoverageFrom=src/community/**",
    "test:community:watch": "jest --watch --testPathPattern=src/community --coverage --collectCoverageFrom=src/community/**",
    "test:course": "jest --testPathPattern=src/course --coverage --collectCoverageFrom=src/course/**",
    "test:course:watch": "jest --watch --testPathPattern=src/course --coverage --collectCoverageFrom=src/course/**",
    "test:notification": "jest --testPathPattern=src/notification --coverage --collectCoverageFrom=src/notification/**",
    "test:notification:watch": "jest --watch --testPathPattern=src/notification --coverage --collectCoverageFrom=src/notification/**",
    "test:place": "jest --testPathPattern=src/place --coverage --collectCoverageFrom=src/place/**",
    "test:place:watch": "jest --watch --testPathPattern=src/place --coverage --collectCoverageFrom=src/place/**",
    "test:reaction": "jest --testPathPattern=src/reaction --coverage --collectCoverageFrom=src/reaction/**",
    "test:reaction:watch": "jest --watch --testPathPattern=src/reaction --coverage --collectCoverageFrom=src/reaction/**",
    "test:search": "jest --testPathPattern=src/search --coverage --collectCoverageFrom=src/search/**",
    "test:search:watch": "jest --watch --testPathPattern=src/search --coverage --collectCoverageFrom=src/search/**",
    "test:subway": "jest --testPathPattern=src/subway --coverage --collectCoverageFrom=src/subway/**",
    "test:subway:watch": "jest --watch --testPathPattern=src/subway --coverage --collectCoverageFrom=src/subway/**",
    "test:theme": "jest --testPathPattern=src/theme --coverage --collectCoverageFrom=src/theme/**",
    "test:theme:watch": "jest --watch --testPathPattern=src/theme --coverage --collectCoverageFrom=src/theme/**"
  },
  "dependencies": {
    "@fastify/static": "^6.11.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.4.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.4.3",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/swagger": "^7.1.11",
    "@nestjs/typeorm": "^10.0.0",
    "@types/cache-manager": "^4.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "blanc-logger": "^1.1.0",
    "cache-manager": "^5.2.3",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "common": "^0.2.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-flow": "^4.1.0",
    "express": "^4.18.2",
    "fastify": "^4.25.2",
    "fp-ts": "^2.16.1",
    "graphql": "^16.9.0",
    "integration": "^0.3.1",
    "joi": "^17.10.1",
    "mongoose": "^7.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.1",
    "nest-winston": "^1.10.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-kakao": "^1.0.1",
    "passport-naver": "^1.0.6",
    "passport-oauth2": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "test": "^3.3.0",
    "typeorm": "^0.3.17",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@automock/jest": "^2.1.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^10.4.15",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "18.11.18",
    "@types/passport-google-oauth20": "^2.0.12",
    "@types/passport-kakao": "^1.0.1",
    "@types/passport-naver": "^1.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "29.3.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "json-summary"
    ],
    "coveragePathIgnorePatterns": [
      "module.ts",
      "dto.ts",
      "entity.ts",
      "guard.ts",
      "middleware.ts",
      "strategy.ts",
      "decorator.ts",
      "pipe.ts",
      "swagger.ts",
      "common/*",
      "config/*",
      "types/*",
      "enum/*",
      "main.ts",
      "app/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    }
  }
}
